{
	"manifest_version": 3,
	"name": "FoE-GBT",
	"description": "FoE Great Building Tracker",
	"version": "1.0",
	"action": {
	  "default_popup": "html/popup.html",
	  "default_icon": "images/theArc_16.png"
	},
	"icons": {
	  "16": "images/theArc_16.png",
	  "48": "images/theArc_48.png",
	  "128": "images/theArc_128.png"
	},

	"background": {
	  },

	"permissions": [
	  "alarms",
	  "storage",
	  "tabs",
	  "notifications",
	  "clipboardWrite"
	],
	"web_accessible_resources": [
	  {
		"resources": [
		  "html/popup2.html",
		  "js/*",
		  "vendor/*",
		  "css/*",
		  "html/*",
		  "images/leroy.png",
          "chrome-extension://nljfmbeipfjdgonopegbbonkphajhgen/html/popup2.html"
		],
		"matches": ["https://*/*"]
	  }
	],
	"content_scripts": [
	  {
		"matches": [
		  "https://*.forgeofempires.com/*"
		],
		"include_globs": [
		  "*forgeofempires.com/game*"
		],
		"run_at": "document_start",
		"js": [
		  "js/inject.js",
		  "js/content.js"
		],
		"css": ["css/content.css"]
	  }
	],
	"externally_connectable": {
	  "matches": ["https://*.forgeofempires.com/*"],
	  "ids": ["*"]
	}
  }
  